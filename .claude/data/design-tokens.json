{
  "version": "1.0.0",
  "aesthetic": "2025-2026 Modern",
  "colors": {
    "primary": {
      "DEFAULT": "#0D9488",
      "50": "#F0FDFA",
      "100": "#CCFBF1",
      "600": "#0D9488",
      "700": "#0F766E",
      "900": "#134E4A"
    },
    "gray": {
      "50": "#F9FAFB",
      "100": "#F3F4F6",
      "500": "#6B7280",
      "900": "#111827"
    },
    "semantic": {
      "success": "#10B981",
      "warning": "#F59E0B",
      "error": "#EF4444",
      "info": "#3B82F6"
    }
  },
  "typography": {
    "fontFamily": {
      "sans": ["Inter", "system-ui", "sans-serif"],
      "heading": ["Cal Sans", "Inter", "sans-serif"],
      "mono": ["JetBrains Mono", "monospace"]
    },
    "fontSize": {
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "2xl": "1.5rem"
    }
  },
  "spacing": {
    "base": "8px",
    "scale": [4, 8, 12, 16, 24, 32, 48, 64, 96]
  },
  "borderRadius": {
    "sm": "6px",
    "md": "8px",
    "lg": "12px",
    "xl": "16px"
  },
  "shadows": {
    "style": "soft colored (NEVER pure black)",
    "md": "0 4px 6px rgba(13, 148, 136, 0.1)",
    "lg": "0 10px 15px rgba(13, 148, 136, 0.1)"
  },
  "iconography": {
    "deprecated": ["Lucide"],
    "recommended": ["AI-generated custom"],
    "rule": "NO generic icon libraries"
  }
}
